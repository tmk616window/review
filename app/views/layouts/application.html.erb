<!DOCTYPE html>
<html>
  <head>
    <title>Review</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  
  <header>
    <div class='container tasks_home'>
      <div class='row justify-content-between'>
        <div class='tasks_home_tab'>
          <h1><%=link_to 'Enjob' ,'#',class:'tasks_home_button'%></h1>
        </div>
        <div class='tasks_sub_tab'>
          <%=link_to 'ログイン' ,'#',class:'tasks_sub_button'%>
          <%=link_to '新規登録' ,'#',class:'tasks_sub_button'%>
        </div>
      </div>
      
    </div>
  </header>
  
  <body>
    <%= yield %>
  </body>
</html>
